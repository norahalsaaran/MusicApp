<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Allsong"
            android:label="@string/allSong"/>
        <activity android:name=".Artist"
            android:label="@string/artist"/>
        <activity android:name=".Album"
            android:label="@string/album" />
        <activity android:name=".Playlist"
            android:label="@string/playlist"/>

    </application>

</manifest>